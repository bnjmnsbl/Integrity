<html lang="en">
<head>
   <%include ../partials/head %>
</head>
    
<body class="container">
    <header>
        <% include ../partials/header %>
    </header>
    
    <main>
        <div class="jumbotron">
      
            <%if (link.length) { %>
            <p>Redirecting to...</p>  
            <a href="<%=link[0].long %>">
            <%=link[0].long %>
            </a>
            
            <% } else { %>
            <p>Not a valid Url</p>
            <% } %>
    
        </div>  
    </main>
    <script>
        setTimeout(
    function(){
        window.open("<%=link[0].long%>")
    },1000);
    </script>
    <footer>
        <% include ../partials/footer %>
    </footer>
</body>
    
</html>
